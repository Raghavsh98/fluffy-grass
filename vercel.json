{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://framer.app https://*.framer.app https://*.framer.website https://*.framer.com;"
        }
      ]
    }
  ]
} 